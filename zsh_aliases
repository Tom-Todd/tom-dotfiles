alias c='clear; neofetch --colors 1 1 1 1 1 6  bar --ascii ~/.config/neofetch/ascii'
alias ca='clear'
alias q='exit'
alias resTouch='sudo zsh ~/.restartMouseDriver.sh'
alias shutdown='sudo systemctl poweroff'
alias ls='ls --color=auto'
alias pacupg='yay -Syu'
alias pacin='yay -S'
alias pacout='yay -R'
alias pmake='makepkg -si'
alias g='git'
alias config='/usr/bin/git --git-dir=$HOME/.cfg/ --work-tree=$HOME'
alias sysen='sudo systemctl enable'
alias sysdi='sudo systemctl disable'

